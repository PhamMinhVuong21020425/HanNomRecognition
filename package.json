{
  "name": "sino-nom",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "next": "next dev",
    "build": "npx ts-node build.ts && next build",
    "start": "cross-env NODE_ENV=production node dist/bin/www.js",
    "dev": "nodemon --exec ts-node src/bin/www.ts",
    "server": "SET DEBUG=sino-nom:* & npm run dev",
    "watch": "tsc -w",
    "lint": "eslint src/**/*.{ts,tsx,json,pug}",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx,json,pug}",
    "format": "prettier --write src/**/*.{ts,tsx,json,pug}",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "migrate:generate": "npm run typeorm -- migration:generate -d ./src/config/data-source.ts",
    "migrate:run": "npm run typeorm -- migration:run -d ./src/config/data-source.ts",
    "migrate:revert": "npm run typeorm -- migration:revert -d ./src/config/data-source.ts",
    "migrate:show": "npm run typeorm -- migration:show -d ./src/config/data-source.ts",
    "tailwind:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/global.css -w"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~4.3.7",
    "dotenv": "^16.4.5",
    "express": "~4.21.1",
    "express-async-handler": "^1.2.0",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.18.1",
    "fs-extra": "^11.2.0",
    "http-errors": "~2.0.0",
    "jet-logger": "^2.0.1",
    "lucide-react": "^0.474.0",
    "morgan": "~1.10.0",
    "next": "^14.2.23",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pug": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-intl": "^7.1.4",
    "react-redux": "^9.2.0",
    "react-spinners": "^0.15.0",
    "redux-persist": "^6.0.0",
    "reflect-metadata": "^0.2.2",
    "sass": "^1.83.4",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.7.0",
    "@prettier/plugin-pug": "^3.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express-mysql-session": "^3.0.6",
    "@types/express-session": "^1.18.1",
    "@types/fs-extra": "^11.0.4",
    "@types/http-errors": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pug": "^2.0.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-pug": "^1.2.5",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "nodemon": "^3.1.7",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.3.3",
    "redux-logger": "^3.0.6",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "description": "Project with Node.js Express and Next.js framework for a major assignment in the Sofware Development Laboratory.",
